/*=============================================================================
=======                            INCLUDES                             =======
=============================================================================*/

/*------ standard includes -------*/
#include <string.h>
/*------ project includes --------*/
#include "Stm32f44xx_gpio.h"
#include "stm32f446xx.h"
/*------ module includes --------*/
#include "Led_InterruptHandle.h"

/*=============================================================================
=======               DEFINES & MACROS FOR GENERAL PURPOSE              =======
=============================================================================*/

/*=============================================================================
=======                       CONSTANTS  &  TYPES                       =======
=============================================================================*/

/*=============================================================================
=======                  PRIVATE FUNCTION DECLARATIONS                  =======
=============================================================================*/
static void delay(uint32_t Value);
/*=============================================================================
=======                              METHODS                            =======
=============================================================================*/

static void delay(uint32_t Value)
{
	Value*=20;
	for(uint32_t i = 0 ; i<= Value; i++);
}
